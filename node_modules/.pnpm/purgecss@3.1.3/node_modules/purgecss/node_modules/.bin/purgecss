#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/junjieguan/git/audiolit/audiolit-js/web/node_modules/.pnpm/purgecss@3.1.3/node_modules/purgecss/bin/node_modules:/Users/junjieguan/git/audiolit/audiolit-js/web/node_modules/.pnpm/purgecss@3.1.3/node_modules/purgecss/node_modules:/Users/junjieguan/git/audiolit/audiolit-js/web/node_modules/.pnpm/purgecss@3.1.3/node_modules:/Users/junjieguan/git/audiolit/audiolit-js/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/junjieguan/git/audiolit/audiolit-js/web/node_modules/.pnpm/purgecss@3.1.3/node_modules/purgecss/bin/node_modules:/Users/junjieguan/git/audiolit/audiolit-js/web/node_modules/.pnpm/purgecss@3.1.3/node_modules/purgecss/node_modules:/Users/junjieguan/git/audiolit/audiolit-js/web/node_modules/.pnpm/purgecss@3.1.3/node_modules:/Users/junjieguan/git/audiolit/audiolit-js/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/purgecss.js" "$@"
else
  exec node  "$basedir/../../bin/purgecss.js" "$@"
fi
